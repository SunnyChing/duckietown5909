<?xml version="1.0"?>
  
<launch>

  <arg name="veh" doc="Name of vehicle. ex: megaman"/>

 <group ns="$(arg veh)">
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
	
    <param name="port" value="/dev/ttyACM1"/>
    <param name="baud" value="19200"/>
  </node>
 </group>
</launch>


